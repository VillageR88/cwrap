      "font-display": "swap"
    },
    {
      "font-family": "FigtreeExtraBold",
      "src": "url('static/fonts/Figtree-ExtraBold.ttf') format('truetype')",
      "font-display": "swap"
    }
  ],
  "root": {
    "--color-titanium-white": "#ffffff",
    "--color-midnight-black": "#000000",
    "--color-cadmium-yellow": "#ffef00"
